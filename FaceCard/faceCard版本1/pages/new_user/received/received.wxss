/* pages/new_user/received/received.wxss */
.container{
  width: 100%;
  height: 100%;
  padding: 180rpx 0 40rpx;
  box-sizing: border-box;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.tip{
  font-size: 28rpx;
  color: #666;
  padding-top: 25rpx;
}
.tip text{
  font-size: 32rpx;
  color: #FF932F;
}
.button{
  width: 420rpx;
  height: 88rpx;
  background: linear-gradient(90deg,rgba(237,57,57,1) 0%,rgba(255,93,82,1) 100%);
  box-shadow: 0rpx 12rpx 20rpx 0rpx rgba(255,209,206,1);
  border-radius: 44rpx;
  line-height: 88rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
  margin-top: 200rpx;
}
.button image{
  width: 650rpx;
  height: 128rpx;
}