<!--pages/mine/setting/setting.wxml-->
<view class="container">
  <view class="cells">

    <view class="cell">
      <view class="cell_content">
        <view class="cell_left">推送设置</view>
        <view class="cell_right">
            <switch color="#FF4142" style="zoom: .7" checked bindchange="switchChange"/>
        </view>
      </view>
    </view>

    <view class="cell">
      <view class="cell_content">
        <view class="cell_left">清除缓存</view>
        <view class="cell_right">
          <!-- <text>暂无缓存</text> -->
          <view bindtap="clearCache">
            <text>60M</text>
            <image class="right" mode="widthFix" src="../../../images/mine_right.png"></image>
          </view>
        </view>
      </view>
    </view>

    <view class="cell">
      <view class="cell_content">
        <view class="cell_left">当前版本</view>
        <view class="cell_right">
          <text>版本号</text>
        </view>
      </view>
    </view>

    <view class="layout" bindtap="layout">退出登录</view>

  </view>
</view>
