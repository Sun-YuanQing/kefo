/* pages/mine/notice/notice.wxss */
page{
  background: #f5f5f5;
}
.list{
  padding: 0 30rpx 30rpx;
}
.list_item{
  width: 100%;
  display: flex;
  justify-content: flex-start;
  box-sizing: border-box;
  background: #fff;
  border-radius: 16rpx;
  margin-top: 20rpx;
  padding: 20rpx 60rpx 30rpx 30rpx;
}
.list_item .icon{
  width: 60rpx;
  height: 60rpx;
}
.list_item .icon image{
  width: 60rpx;
}
.list_item .title{
  font-size: 30rpx;
}
.list_item .content_wrap{
  font-size: 26rpx;
  margin-left: 26rpx;
}
.list_item .content{
  color: #666;
  line-height: 1.6;
  margin: 18rpx 0;
}
.list_item .other{
  color: #999;
}