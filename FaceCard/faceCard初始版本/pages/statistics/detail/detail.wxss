page{
  background: #F5F5F5;
  width: 100%;
  height: 100%;
   box-sizing: border-box;
   padding: 20rpx 30rpx;
}
.detailBox{
  background: #fff;
  width: 100%;
  height: 100%;
  border-radius: 16rpx;
}
.img_text{
  text-align: center;
  padding-top: 80rpx;
}
.icon_img{
  width: 120rpx;
  height: 120rpx;
}
.textMoney{
  font-size: 44rpx;
}
.containBox{
  width: 100%;
  box-sizing: border-box;
  padding: 60rpx 60rpx 0;
}
.line_text{
  display: flex;
  margin-bottom: 28rpx;
}
.left_title{
  font-size: 28rpx;
  color: #999999;
  margin-right: 40rpx;
  line-height: 45rpx;
}
.right_name{
  font-size: 28rpx;
  width: calc(100% - 152rpx);
  line-height: 45rpx;
}