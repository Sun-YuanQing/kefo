/* pages/card_bag/menber_center/menber_center.wxss */
page{
  background: #f5f5f5;
}
.container{
  width: 100%;
  height: 100%;
}
.nav_bar{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 105rpx;
  padding: 0 30rpx;
  margin: 20rpx 0;
  font-size: 32rpx;
  background: #fff;
}
.nav_bar_left{
   display: flex;
   align-items: center;
}
.nav_bar_left image{
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
}
.nav_bar_right{
  width: 16rpx;
  height: 30rpx;
}

.my_card{
  background: #fff;
  padding: 0 30rpx 40rpx;
  min-height: calc(100% - 180rpx);
}
.my_card .title{
  font-size: 32rpx;
  font-weight: 700;
  line-height: 4;
}
.other_content{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 60rpx;
}
.other_content view:first-child{
  font-size: 34rpx;
}
.other_content  image{
  width: 10rpx;
  height: 20rpx;
  margin-left: 16rpx
}
.card_money{
  font-size: 26rpx;
}
.card_item{
  width: 100%;
  height: 180rpx;
  box-sizing: border-box;
  padding: 30rpx 40rpx;
  color: #fff;
  font-size: 26rpx;
  border-radius: 16rpx;
  margin-bottom: 30rpx;
}

