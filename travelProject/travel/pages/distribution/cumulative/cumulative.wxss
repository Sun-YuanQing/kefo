/* pages/distribution/cumulative/cumulative.wxss */
.content-box {
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 30rpx;
}

.dataTime {
  background: #F5F5F5;
  border-radius: 30rpx;
  font-size: 28rpx;
  font-weight: 800;
  width: 180rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.sell-price {
  font-size: 24rpx;
  color: #999;
  margin-top: 14rpx;
}

.gray {
  background: #F5F5F5;
  width: 100%;
  height: 20rpx;
}

.list-box {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.list-item {
  padding: 28rpx 0;
  width: 100%;
  border-bottom: 1rpx solid #EEEEEE;
}

.item-name {
  font-size: 28rpx;
  font-weight: 800;
}

.price-line {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 30rpx;
}

.successPrice {
  font-size: 28rpx;
  color: #666;
}
.successPrice text{
  margin-left: 12rpx;
  font-weight: 800;
  color: #000;
}

.addPrice {
  font-size: 28rpx;
  color: #FF4B35;
  font-weight: 800;
}

.timeStyle {
  font-size: 24rpx;
  color: #666;
  margin-top: 12rpx;
}