<!--pages/address/userExtract/userExtract.wxml-->
<!-- 填写提货联系人 -->
<view>
  <view class="rgbccc"></view>
  <view class="padding30">
    <view class="f-row-start  boderBottom-solid1px wh h-center">
      <view class="address-label">联系人</view>
      <input class="address-input margin-left64 width10rem" placeholder-class="phcolor" value="{{ userName }}" bindinput="bindUserName" placeholder="请输入提货人真实姓名"></input>
    </view>
    <view class="f-row-start  boderBottom-solid1px wh h-center">
      <view class="address-label">手机号</view>
      <input class="address-input margin-left64 width10rem" placeholder-class="phcolor" maxlength="11" value="{{ telNumber }}" bindinput="bindTelNumber" placeholder="请输入提货人手机号"></input>
    </view>

  </view>
  <view class="purchase-box">
    <view class="purchase" bindtap="onAddress">提交信息</view>
  </view>
  <i-message id="message" />
</view>